  "main": "index.js",
  "scripts": {
    "build:user-service": "cd microservices/user-service && npm install",
    "build:questions-service": "cd microservices/questions-repo && npm install",
    "build:sync-questions-service": "cd microservices/sync-questions-service && npm install && cd functions && npm install",
    "build:auth-service": "cd microservices/auth && npm install",
    "build:front-end-service": "cd microservices/front-end && npm install",
    "build:all": "npm-run-all build:user-service build:questions-service build:sync-questions-service build:auth-service build:front-end-service",
    
    "dev:user-service": "cd microservices/user-service && npm run start",
    "dev:questions-service": "cd microservices/questions-repo && npm run serverstart",
    "dev:sync-questions-service": "cd microservices/sync-questions-service/functions && npm run dev",
    "dev:auth-service": "cd microservices/auth && npm run dev",
    "dev:front-end-service": "cd microservices/front-end && npm run dev",
    "dev:all": "npm-run-all --parallel dev:user-service dev:questions-service dev:auth-service dev:front-end-service"
  },
